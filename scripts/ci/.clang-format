BasedOnStyle: Google
---
Language: Cpp
ColumnLimit: 80
DerivePointerAlignment: false
PointerAlignment: Left
IncludeCategories:
  # Note that the "main" header is priority 0
  # The priority is assigned to first match in the ordered list
  # C standard libraries
  - Regex:       '<(arpa/|linux/can/|linux/|netinet/|net/if|sys/)?[^\./]*\.h>'
    Priority:    1
  # C++ standard libraries
  - Regex:       '<[^/\./]*>'
    Priority:    2
  - Regex:      '<Qt[^/\./]*>'
    Priority:    4
  # Experimental or other system libraries
  - Regex:       '<'
    Priority:    3
  # Test libs
  - Regex:       '"(gtest|gmock|third_party)/'
    Priority:    6
  # QCraft libs
  - Regex:       '^"onboard/'
    Priority:    7
  - Regex:       '^"offboard/'
    Priority:    8
  - Regex:       '^"(cyber_modules|experimental)/'
    Priority:    9
  # The rest
  - Regex:       '.*'
    Priority:    5
...

